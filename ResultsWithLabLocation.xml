							<templateId root="2.16.840.1.113883.10.20.22.4.1"/>
							<id root="1.3.6.1.4.1.22812.20.1.1.4.5" extension="1"/>
							<code code="24357-6" codeSystem="2.16.840.1.113883.6.1"/>
							<statusCode code="completed"/>
							<effectiveTime xsi:type="IVL_TS">
